{
  "name": "<%= pkg %>",
  "version": "0.0.0-development",
  "description": "<%= description %>",
  "main": "dist/index.js",
  "typings": "build/index.d.ts",
  "module": "build/index.js",
  "repository": "https://github.com/<%= githubUsername %>/<%= repoName %>.git",
  "license": "MIT",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "watch:parcel": "parcel watch src/index.ts --target node",
    "watch:tsc": "tsc --watch",
    "build:parcel": "parcel build build/index.js --target node",
    "build:tsc": "tsc",
    "start:bundle": "run-p watch:tsc watch:parcel",
    "start": "nodemon --watch src -e ts,tsx --exec 'yarn dev'",
    "lint": "tslint -p tsconfig.json",
    "prebuild": "rimraf lib",
    "build:prod": "NODE_ENV=production babel src --out-dir lib",
    "build": "run-s build:tsc build:parcel test",
    "clean": "rimraf build dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "docs:clean": "rimraf docs",
    "docs:create": "typedoc --out './docs' --readme './README.md' --module 'commonjs'",
    "docs:deploy": "ts-node ./tools/gh-pages-publish",
    "docs:update": "run-s docs:clean docs:create docs:deploy",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "@types/rimraf": "^2.0.2",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.1",
    "parcel-plugin-typescript": "^1.0.0",
    "prettier": "^1.14.3",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.16",
    "travis-deploy-once": "^5.0.8",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3"
  }
}
