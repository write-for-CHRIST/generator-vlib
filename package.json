{
  "name": "generator-vlib",
  "version": "1.0.0",
  "description": "Generator a library with well-configured Open Source tools",
  "homepage": "https://github.com/write-for-CHRIST/generator-vlib",
  "author": {
    "name": "nampdn",
    "email": "nampdn@protonmail.com"
  },
  "files": [
    "generators"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  },
  "main": "generators/index.js",
  "keywords": [
    "yeoman",
    "yo",
    "generator",
    "library",
    "automate",
    "devops",
    "module",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "coveralls": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.24.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.11.1",
    "semantic-release": "^15.9.17",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "repository": "write-for-CHRIST/generator-vlib",
  "license": "MIT"
}
